{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".svelte-kit/**"]
    },
    "lint": {
      "dependsOn": ["^lint"],
      "inputs": ["apps/*/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "inputs": ["apps/*/**", "packages/*/**", ".env*"]
    }
  }
}